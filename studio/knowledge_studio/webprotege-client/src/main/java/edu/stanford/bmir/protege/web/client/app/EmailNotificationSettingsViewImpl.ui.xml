<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="wp" type="edu.stanford.bmir.protege.web.resources.WebProtegeClientBundle"/>
    <g:HTMLPanel addStyleNames="{wp.style.form}">
        <div class="{wp.style.formGroup}">
            <g:Label addStyleNames="{wp.style.formLabel}" text="Email Notifications"/>
            <div class="{wp.style.formCheckBox}">
                <g:CheckBox ui:field="emailNotificationsEnabledCheckBox"
                            text="Email notifications enabled"/>
            </div>
        </div>

    </g:HTMLPanel>
</ui:UiBinder>