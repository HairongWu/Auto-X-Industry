<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="wp" type="edu.stanford.bmir.protege.web.resources.WebProtegeClientBundle"/>
    <g:HTMLPanel>
        <g:HTMLPanel addStyleNames="{wp.style.formGroup}">
            <g:Label text="When entity" addStyleNames="{wp.style.formLabel}"/>
            <g:SimplePanel ui:field="criteriaListViewContainer"/>
        </g:HTMLPanel>
        <g:HTMLPanel addStyleNames="{wp.style.formGroup}">
            <g:Label text="Use IRI prefix" addStyleNames="{wp.style.formLabel}"/>
            <g:TextBox ui:field="iriPrefixField"/>
        </g:HTMLPanel>
    </g:HTMLPanel>
</ui:UiBinder>
