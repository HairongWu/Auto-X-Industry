<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="wp" type="edu.stanford.bmir.protege.web.resources.WebProtegeClientBundle"/>
    <ui:style>
        .container {
            width: 100%;
        }
    </ui:style>
    <g:HTMLPanel>
        <g:HTMLPanel addStyleNames="{wp.style.formGroup}">
            <g:SimplePanel ui:field="prefixSettingsViewContainer" addStyleNames="{style.container}"/>
        </g:HTMLPanel>
        <g:HTMLPanel addStyleNames="{wp.style.formGroup}">
            <g:Label text="IRI Suffix Settings" addStyleNames="{wp.style.formLabel}"/>
            <g:SimplePanel ui:field="suffixSettingsViewContainer" addStyleNames="{style.container}"/>
        </g:HTMLPanel>
    </g:HTMLPanel>
</ui:UiBinder>
