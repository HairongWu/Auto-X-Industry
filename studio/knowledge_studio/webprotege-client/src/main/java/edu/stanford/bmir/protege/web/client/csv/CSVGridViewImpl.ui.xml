<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:c='urn:import:com.google.gwt.user.cellview.client'>
    <ui:style>
        @external .web-protege-highlighted-table-column, .web-protege-mapped-table-column;
        .outer {
            background-color: white;
        }
        .outer .web-protege-highlighted-table-column {
            background-color: #f3f3ff
        }

        .outer .web-protege-mapped-table-column {
            font-weight: bold !important;
            background-color: #efffef;
        }
    </ui:style>
    <g:HTMLPanel width="100%" addStyleNames="{style.outer}">
        <c:DataGrid ui:field="dataGrid" width="100%" height="200px"/>
    </g:HTMLPanel>
</ui:UiBinder>