<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="wp" type="edu.stanford.bmir.protege.web.resources.WebProtegeClientBundle"/>
    <ui:style>
        .main {
            margin: 5px;
            white-space: nowrap;
        }
        .label {
            font-weight: bold;
        }
    </ui:style>
    <g:HTMLPanel addStyleNames="{style.main}">
        <g:Label ui:field="label" addStyleNames="{style.label} {wp.style.formLabel}"/>  <g:Anchor ui:field="selectAllButton" text="Select All"/> | <g:Anchor ui:field="selectNoneButton" text="Select None"/>
    </g:HTMLPanel>
</ui:UiBinder>
