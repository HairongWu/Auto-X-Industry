    <ui:with field="msg" type="edu.stanford.bmir.protege.web.client.Messages"/>
    <g:HTMLPanel>
        <g:Button ui:field="logoutButton" addStyleNames="{wp.toolbar.toolbarButton}" text="{msg.signOut}"/>
    </g:HTMLPanel>
</ui:UiBinder>