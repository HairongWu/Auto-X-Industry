<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style>
        .container {
            padding-top: 3px;
            padding-bottom: 3px;
        }
        .checkBox {
            padding-left: 7px;
            padding-right: 7px;
        }
    </ui:style>
    <g:HTMLPanel>
        <g:HTMLPanel addStyleNames="{style.container}">
            <g:CheckBox ui:field="classCheckBox" text="Class" addStyleNames="{style.checkBox}"/>
            <g:CheckBox ui:field="individualCheckBox" text="Individual" addStyleNames="{style.checkBox}"/>
            <g:CheckBox ui:field="datatypeCheckBox" text="Datatype" addStyleNames="{style.checkBox}"/>
        </g:HTMLPanel>
        <g:HTMLPanel addStyleNames="{style.container}">
            <g:CheckBox ui:field="objectPropertyCheckBox" text="Object property" addStyleNames="{style.checkBox}"/>
            <g:CheckBox ui:field="dataPropertyCheckBox" text="Data property" addStyleNames="{style.checkBox}"/>
            <g:CheckBox ui:field="annotationPropertyCheckBox" text="Annotation property" addStyleNames="{style.checkBox}"/>
        </g:HTMLPanel>

    </g:HTMLPanel>
</ui:UiBinder>