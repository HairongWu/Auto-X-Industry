{
  "name": "Auto-X Studio",
  "description": "The first open source project of full-fledged digital twin and AI solution for industrial scenarios",
  "keywords": [
    "data annotation",
    "data labeling",
    "digital twin",
    "AI solution",
    "industrial scenarios"
  ],
  "website": "",
  "repository": "https://github.com/HairongWu/Auto-X-Industry",
  "logo": "",
  "stack": "container",
  "env": {
    "DISABLE_SIGNUP_WITHOUT_LINK": {
      "description": "Disable signup for users without invite link",
      "value": "0",
      "required": false
    },
    "USERNAME": {
      "description": "Username(email) for default user",
      "value": "",
      "required": false
    },
    "PASSWORD": {
      "description": "Password for default user",
      "value": "",
      "required": false
    }
  },
  "build": {
    "skip": true
  },
  "hooks": {
    "prebuild": {
      "commands": [
        "docker build -f Dockerfile.cloudrun -t $IMAGE_URL ."
      ]
    }
  }
}
