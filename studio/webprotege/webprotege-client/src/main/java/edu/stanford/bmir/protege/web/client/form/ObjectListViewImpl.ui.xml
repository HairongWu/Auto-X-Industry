<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:editor="urn:import:edu.stanford.bmir.protege.web.client.editor">
    <ui:with field="wp" type="edu.stanford.bmir.protege.web.resources.WebProtegeClientBundle"/>
    <ui:style>
        .container {
            display: flex;
            flex-direction: column;
            align-items: stretch;
        }
    </ui:style>
    <g:HTMLPanel>
        <g:HTMLPanel ui:field="elementDescriptorViewContainer" addStyleNames="{style.container}"/>
        <g:Button ui:field="addObjectButton" text="Add field" addStyleNames="{wp.buttons.button} {wp.buttons.secondaryButton} {wp.buttons.smallPageButton}"/>
    </g:HTMLPanel>
</ui:UiBinder>
