<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:primitive="urn:import:edu.stanford.bmir.protege.web.client.primitive">
    <ui:style>
        .field {
            min-width: 170px;
            max-width: 170px;
        }
    </ui:style>
    <g:HTMLPanel>
        <primitive:PrimitiveDataEditor
                ui:field="entityField"
                addStyleNames="{style.field}"
                placeholder="Enter an entity name" classesAllowed="true" namedIndividualsAllowed="true"/>
    </g:HTMLPanel>
</ui:UiBinder>
