<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="wp" type="edu.stanford.bmir.protege.web.resources.WebProtegeClientBundle"/>
    <g:HTMLPanel>
        <g:HTMLPanel addStyleNames="{wp.style.formGroup}">
            <g:HTMLPanel>
                <g:Button text="Set as project default" ui:field="setAsProjectDefaultButton"
                          addStyleNames="{wp.buttons.smallPageButton} {wp.buttons.secondaryButton}"/>
            </g:HTMLPanel>
        </g:HTMLPanel>
        <g:Label addStyleNames="{wp.style.formHelpText}" text="You can make the above tabs the default set of tabs for this project for other users that have access to the project."/>
    </g:HTMLPanel>
</ui:UiBinder>